import com.jxl.goocal.views.LoginForm;import com.jxl.shuriken.core.Collection;		class test_LoginForm extends MovieClip{	private static var inst:test_LoginForm;		private var root:MovieClip;	private var mc:LoginForm;		public static function getInstance(p_root:MovieClip):test_LoginForm	{		if(inst == null) inst = new test_LoginForm(p_root);		return inst;	}		public function test_LoginForm(p_root)	{		root = p_root;	}		public function init():Void	{		Stage.scaleMode = "noScale";		Stage.align = "TL";		//fscommand2("FullScreen", true);				mc = LoginForm(root.attachMovie(LoginForm.SYMBOL_NAME, "mc", 0));					}}